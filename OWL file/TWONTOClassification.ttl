               owl:equivalentClass [ owl:intersectionOf ( :property
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty [ owl:inverseOf :hasPropertyState
                                                                           ] ;
                                                            owl:someValuesFrom owl:Thing
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:label "property state"@en .


