###  http://www.toronto.ca/twonto#OReg220PressureVessel
:OReg220PressureVessel rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( :vesselOrTank
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :hasMaxAllowableWorkingPressure ;
                                                                    owl:someValuesFrom [ owl:intersectionOf ( :pressure
                                                                                                              [ rdf:type owl:Restriction ;
                                                                                                                owl:onProperty :hasUnitOfMeasure ;
                                                                                                                owl:hasValue :psi
                                                                                                              ]
                                                                                                              [ rdf:type owl:Restriction ;
                                                                                                                owl:onProperty :hasNumericValue ;
                                                                                                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                                                     owl:onDatatype xsd:float ;
                                                                                                                                     owl:withRestrictions ( [ xsd:minExclusive "15.0"^^xsd:float
                                                                                                                                                            ]
                                                                                                                                                          )
                                                                                                                                   ]
                                                                                                              ]
                                                                                                            ) ;
                                                                                         rdf:type owl:Class
                                                                                       ]
                                                                  ]
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :isDesignedToHold ;
                                                                    owl:allValuesFrom [ rdf:type owl:Class ;
                                                                                        owl:unionOf ( :gaseousMaterial
                                                                                                      :liquid
                                                                                                      :vapour
                                                                                                    )
                                                                                      ]
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] .


