# Pump SHAPE
tw:PumpSet_default_shape a sh:NodeShape ;
  sh:targetClass tw:pumpset ;
  sh:property [
    sh:path tw:orientation ;
    sh:name "Orientation";
    sh:datatype xsd:string ;
    sh:in ( "Horizontal" "Vertical" "Angled" "Inverted" ) ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:message "Value must be a string and should be found in the standard / dropdown value list" ;
    sh:severity sh:Warning ;
    sh:order 50 ;
  ] ;
  sh:property [
    sh:path tw:variable_speed ;
    sh:name "Variable Speed";
    sh:datatype xsd:string ;
    sh:in ("True" "False") ;
    sh:message "Value must be a string and should be found in the standard / dropdown value list" ;
    sh:severity sh:Warning ;
    sh:order 60 ;
  ] ;
  
	sh:property [
		sh:path tw:pump_type ; #4Dev: the property label on the form "NEMA enclosure type" is the RDFS:label of this OWL dataProperty.
		sh:name "Pump Type";
		sh:datatype xsd:string ;
		sh:in ( "Dynamic, Centrifugal" "Dynamic, Axial" "Reciprocating, Piston or Plunger" "Reciprocating, Diaphram" "Rotary, Vane" "Rotary, Piston" "Screw Pump" "Gear Pump" ) ;
		sh:maxCount 1 ; sh:minCount 1 ;
		sh:message "Value must be a string and should be found in the standard / dropdown value list" ;
    	sh:severity sh:Warning ; #4Dev: this line tells you to allow alternatives
		sh:order 50 ;
	] ;

	sh:property [
		sh:class tw:max_RPM ;
		sh:name "Max RPM";
		sh:minCount 1 ; sh:maxCount 1 ; 
		# a functional property, but only for motors
		sh:severity sh:Violation ;
		sh:order 10 ;
	] ;

	sh:property [
		sh:class tw:rated_flow_at_max_RPM ;
		sh:name "Rated flow at Max RPM";
		sh:minCount 1 ; sh:maxCount 1 ; 
		# a functional property, but only for motors
		sh:severity sh:Violation ;
		sh:order 10 ;
	] ;

	sh:property [
		sh:class tw:pump_head_at_rated_flow ;
		sh:name "Pump Head at Rated Flow";
		sh:minCount 1 ; sh:maxCount 1 ; 
		# a functional property, but only for motors
		sh:severity sh:Violation ;
		sh:order 10 ;
	] ;

	sh:property [
		sh:path tw:submersible ;
		sh:name "Submersible";
		sh:datatype xsd:boolean ;
		sh:maxCount 1 ; sh:minCount 1 ;#functional property
		sh:severity sh:Violation ;
		sh:order 30 ;
	] ;

	sh:property [
		sh:path tw:pump_drive ; #4Dev: the property label on the form "NEMA enclosure type" is the RDFS:label of this OWL dataProperty.
		sh:name "Pump Drive";
		sh:datatype xsd:string ;
		sh:in ( "Belt Drive" "Direct Drive" "Gear Drive" "Flexible" "Chain Drive" "Hydraulic" ) ;
		sh:maxCount 1 ; sh:minCount 1 ;
		sh:message "Value must be a string and should be found in the standard / dropdown value list" ;
    	sh:severity sh:Warning ; #4Dev: this line tells you to allow alternatives
		sh:order 50 ;
	] ;

	sh:property [
		sh:path tw:pump_drive_type ; #4Dev: the property label on the form "NEMA enclosure type" is the RDFS:label of this OWL dataProperty.
		sh:name "Pump Drive Type";
		sh:datatype xsd:string ;
		sh:in ( "Electic Motor" "Engine" ) ;
		sh:maxCount 1 ; sh:minCount 1 ;
		sh:message "Value must be a string and should be found in the standard / dropdown value list" ;
    	sh:severity sh:Warning ; #4Dev: this line tells you to allow alternatives
		sh:order 50 ;
	] ;

	sh:property [
		sh:path tw:hasSealedOrShieldedBearing ;
		sh:name "has sealed or shielded bearing";
		sh:datatype xsd:boolean ;
		sh:maxCount 1 ; sh:minCount 1 ;#functional property
		sh:severity sh:Violation ;
		sh:order 30 ;
	] ;

	sh:property [
		sh:path tw:isMotorIntegrated ;
		sh:name "Is Motor Integrated";
		sh:datatype xsd:boolean ;
		sh:maxCount 1 ; sh:minCount 1 ;#functional property
		sh:severity sh:Violation ;
		sh:order 30 ;
	] ;

	sh:property [
		sh:path tw:seal_type ; #4Dev: the property label on the form "NEMA enclosure type" is the RDFS:label of this OWL dataProperty.
		sh:name "Seal Type";
		sh:datatype xsd:string ;
		sh:in ( "Mechanical Seal""Packaging Seal" "Lip Seal" "Cartridge Seal" ) ;
		sh:maxCount 1 ; sh:minCount 1 ;
		sh:message "Value must be a string and should be found in the standard / dropdown value list" ;
    	sh:severity sh:Warning ; #4Dev: this line tells you to allow alternatives
		sh:order 50 ;
	] ;
	
. ### End of Shape ###=================================================

# MOTOR SHAPE

tw:Motor_defaultShape
	a sh:NodeShape ;
	sh:targetClass tw:motor ;    
	sh:property [
		sh:path tw:hasSpecification ;
		sh:class tw:ratedPowerOutput ;
		sh:name "rated power output";
		sh:minCount 1 ; sh:maxCount 1 ; 
		# a functional property, but only for motors
		sh:node tw:powerSpec_nodeShape ;
		sh:severity sh:Violation ;
		sh:order 10 ;
	] ;
	#4Dev: can have more than 1 of the property:
	sh:property [
		sh:path tw:hasSpecification ;
		sh:class tw:nominalInputVoltage ;
		sh:name "nominal input voltage";
		sh:minCount 1 ; 
		sh:node tw:voltageSpec_nodeShape ;
		sh:severity sh:Violation ;
		sh:order 20 ;
	] ;
	sh:property [
		sh:path tw:hasSealedOrShieldedBearing ;
		sh:name "has sealed or shielded bearing";
		sh:datatype xsd:boolean ;
		sh:maxCount 1 ; sh:minCount 1 ;#functional property
		sh:severity sh:Violation ;
		sh:order 30 ;
	] ;
	sh:property [
		sh:path eidf:uses_bearing_lubricant_type ;
		sh:name "uses bearing lubricant type";
		sh:datatype xsd:string ;
		sh:in ( "oil" "grease" ) ;
		sh:maxCount 1 ;
		sh:severity sh:Violation ;
		sh:order 40 ;
	] ;

	sh:or ( #4Dev: using the material implication rule to express and if-then statement;
			[
              sh:not [  #: this is the if block;
				sh:path tw:hasSealedOrShieldedBearing ; 
				sh:hasValue false ;
				]
            ]
			[	
				sh:path eidf:uses_bearing_lubricant_type ;
				sh:minCount 1 ;
				sh:severity sh:Violation ;
			]  
	) ;

	sh:property [
		sh:path tw:has_NEMA_enclosureType ; #4Dev: the property label on the form "NEMA enclosure type" is the RDFS:label of this OWL dataProperty.
		sh:name "NEMA enclosure type";
		sh:datatype xsd:string ;
		sh:in ( "ODP" "TEFC" "TENV" "TEAO" "TEWD" "EXPL" "HAZ" ) ;
		sh:maxCount 1 ; sh:minCount 1 ;
		sh:message "Value must be a string and should be found in the standard / dropdown value list" ;
    	sh:severity sh:Warning ; #4Dev: this line tells you to allow alternatives
		sh:order 50 ;
	] ;

	sh:property [
		sh:maxCount 1 ; sh:minCount 1 ;
		sh:path tw:has_NEMA_enclosureType ;
		sh:severity sh:Violation ;
	] ;

    sh:property [
		sh:path tw:has_NEMA_frameType ;
		sh:name "NEMA frame rating";
		sh:datatype xsd:string ;
		sh:in ( "42" "48" "48H" "56" "56H" "56HZ" "66" "143T" "145T" "146AT" "148AT" "149AT" "1412AT" "182" "184" "182T" "184T" "182AT" "L182ACY" "186ACY" "186AT" "L186AT" "189AT" "203#" "204#" "213" "215" "213T" "215T" "219AT" "2110AT" "224#" "225#" "254#" "254U" "256U" "254T" "256T" "284#" "284U" "286U" "284T" "286T" "284TS" "286TS" "324#" "326#" "324U" "326U" "324T" "326T" "324TS" "326TS" "364#" "364S#" "365#" "364U" "365U" "364T" "365T" "364TS" "365TS" "404T" "405T" "404TS" "405TS" "404U" "405U" "444T" "445T" "447T&&" "444TS" "447TS&&" "445TS" "444U" "445U" "449T" "449TS" "#Other" ) ;
		sh:message "Value must be a string and should be found in the standard / dropdown value list" ;
    	sh:severity sh:Warning ;
		sh:order 60 ;
	] ;

	sh:property [
		sh:path tw:has_NEMA_frameType ;
		sh:maxCount 1 ; sh:minCount 1 ;
		sh:severity sh:Violation ;
	] 

.  ### End of Shape ###=================================================

# BOILER SHAPE

tw:Boiler_defaultShape
	a sh:NodeShape ;
	sh:targetClass tw:boiler ;
	
	sh:property [
		sh:path tw:hasSpecification ;
		sh:class tw:maxHoldingCapacity ; #checked
		sh:name "boiler capacity" ;
		sh:minCount 1 ; 
		sh:node tw:holdingCapacitySpec_nodeShape ; #checked
		sh:severity sh:Violation ;
		sh:order 10 ;
	] ;
	sh:property [ 
		sh:path eidf:has_boiler_energy_source ;
		sh:name "boiler medium" ;
		sh:datatype xsd:string ;
		sh:in ( "steam" "thermal oil" "water" "water glycol mixture" ) ;
		sh:severity sh:Warning ;
		sh:minCount 1 ; sh:maxCount 1 ;
		sh:order 20 ;
	] ;
	sh:property [
		sh:path tw:hasSpecification ;
		sh:class tw:maxMediumTemperature ; #checked
		sh:name "maximum temperature" ;
		sh:minCount 1 ; 
		sh:node tw:temperatureSpec_nodeShape ; #checked
		sh:severity sh:Violation ;
		sh:order 30 ;
	] ;
	sh:property [
		sh:path tw:hasSpecification ;
		sh:class tw:maxOperatingPressure ; #checked
		sh:name "maximum operating pressure" ;
		sh:minCount 1 ; 
		sh:node tw:pressureSpec_nodeShape ; #checked
		sh:severity sh:Violation ;
		sh:order 40 ;
	] ;
	sh:property [
		sh:path tw:hasSpecification ;
		sh:class tw:boilerHeatingSurfaceArea ; #checked
		sh:name "heating surface area" ;
		sh:minCount 1 ; 
		sh:node tw:surfaceAreaSpec_nodeShape ; #checked
		sh:severity sh:Violation ;
		sh:order 50 ;
	] ;

. # End of Shape #=================================================


