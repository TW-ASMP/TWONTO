@prefix tw: <http://www.toronto.ca/TWONTO#>.
@prefix eidf: <http://www.toronto.ca/EIDF#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.

# UPS SHAPE
tw:UPS_default_shape a sh:NodeShape ;
  sh:targetClass tw:UPS ;
  sh:property [
    sh:path tw:has_ups_backup ;
    sh:name "UPS Backup for Fire, Life Safety, Elevator Load or Their Control?";
    sh:datatype xsd:boolean ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:severity sh:Violation ;
    sh:order 30 ;
  ] ;
  sh:property [
    sh:class tw:battery_capacity ;
    sh:name "Battery Capacity ";
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:order 10 ;
  ] ;
  sh:property [
    sh:class tw:output_voltage ;
    sh:name "Output Voltage";
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:order 10 ;
  ] ;
  sh:property [
    sh:class "battery_type";
    sh:name "Battery Type" ;
    sh:datatype xsd:string ;
    sh:in ( "piping in fire protection system" "piping in heating system" "piping in refrigeration system" "compressed air piping" "hot oil piping" "buried water piping") ;
    sh:severity sh:Warning ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:order 20 ;
  ] ;
  sh:property [
    sh:path tw:physically_integrated ;
    sh:name "Physically Integrated with Charger";
    sh:datatype xsd:boolean ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:severity sh:Violation ;
    sh:order 30 ;
  ] ;
.

