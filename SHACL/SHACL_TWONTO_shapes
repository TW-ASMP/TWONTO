# UPS SHAPE

tw:UPS_defaultShape
	a sh:NodeShape ;
	sh:targetClass tw:UPS ;    
	sh:property [
		sh:path tw:hasUPSBackup ;
		sh:name "UPS Backup for Fire, Life Safety, Elevator Load or Their Control?";
		sh:datatype xsd:boolean ;
		sh:maxCount 1 ; sh:minCount 1 ;#functional property
		sh:severity sh:Violation ;
		sh:order 30 ;
	] ;

	sh:property [
		sh:class tw:battery_capacity  ;
		sh:name "Battery Capacity ";
		sh:minCount 1 ; sh:maxCount 1 ; 
		sh:severity sh:Violation ;
		sh:order 10 ;
	] ;

	sh:property [
		sh:class tw:output_voltage  ;
		sh:name "Output Voltage";
		sh:minCount 1 ; sh:maxCount 1 ; 
		sh:severity sh:Violation ;
		sh:order 10 ;
	] ;

	sh:property [ 
		sh:class "battery_type"
		sh:name "Battery Type" ;
		sh:datatype xsd:string ;
		sh:in ( "piping in fire protection system" "piping in heating system" "piping in refrigeration system" "compressed air piping" "hot oil piping" "buried water piping") ;
		sh:severity sh:Warning ;
		sh:minCount 1 ; sh:maxCount 1 ;
		sh:order 20 ;
	] ;

	sh:property [
		sh:path tw:physicallyIntegrated ;
		sh:name "Physically Integrated with Charger";
		sh:datatype xsd:boolean ;
		sh:maxCount 1 ; sh:minCount 1 ;#functional property
		sh:severity sh:Violation ;
		sh:order 30 ;
	] ;

.

# Generator SHAPE

tw:Generator_defaultShape
	a sh:NodeShape ;
	sh:targetClass tw:generator ;    
	sh:property [
		sh:path tw:hasGeneratorBackup ;
		sh:name "Generator Backup for Fire, Life Safety, Elevator Load or Their Control?";
		sh:datatype xsd:boolean ;
		sh:maxCount 1 ; sh:minCount 1 ;#functional property
		sh:severity sh:Violation ;
		sh:order 30 ;
	] ;

	sh:property [
		sh:path tw:isMobile ;
		sh:name "Is Mobile";
		sh:datatype xsd:boolean ;
		sh:maxCount 1 ; sh:minCount 1 ;#functional property
		sh:severity sh:Violation ;
		sh:order 30 ;
	] ;

	sh:property [
		sh:class tw:power_rating  ;
		sh:name "Power Rating ";
		sh:minCount 1 ; sh:maxCount 1 ; 
		sh:severity sh:Violation ;
		sh:order 10 ;
	] ;

	sh:property [
		sh:class tw:voltage_rating  ;
		sh:name "Voltage Rating ";
		sh:minCount 1 ; sh:maxCount 1 ; 
		sh:severity sh:Violation ;
		sh:order 10 ;
	] ;

	sh:property [ 
		sh:class "generator_drive"
		sh:name "Generator Drive" ;
		sh:datatype xsd:string ;
		sh:in ( "Sealed / Valve Regulated Lead Acid" "Flooded / Vented Lead Acid" "NiCad" "Li-ion") ;
		sh:severity sh:Warning ;
		sh:minCount 1 ; sh:maxCount 1 ;
		sh:order 20 ;
	] ;

	sh:property [
		sh:path tw:brushlessGenerator ;
		sh:name "Has Brushless Generator";
		sh:datatype xsd:boolean ;
		sh:maxCount 1 ; sh:minCount 1 ;#functional property
		sh:severity sh:Violation ;
		sh:order 30 ;
	] ;

	sh:property [
		sh:path tw:hasTestLoadElectricalConnector ;
		sh:name "Has Test Load Electrical Connector";
		sh:datatype xsd:boolean ;
		sh:maxCount 1 ; sh:minCount 1 ;#functional property
		sh:severity sh:Violation ;
		sh:order 30 ;
	] ;


	sh:property [
		sh:path tw:hasSealedOrShieldedBearing ;
		sh:name "Has Sealed or Shielded Bearing";
		sh:datatype xsd:boolean ;
		sh:maxCount 1 ; sh:minCount 1 ;#functional property
		sh:severity sh:Violation ;
		sh:order 30 ;
	] ;

	sh:property [ 
		sh:class "generator_drive_coupling"
		sh:name "Generator Drive Coupling" ;
		sh:datatype xsd:string ;
		sh:in ( "Water" "Glycol" "Steam") ;
		sh:severity sh:Warning ;
		sh:minCount 1 ; sh:maxCount 1 ;
		sh:order 20 ;
	] ;
	
.

