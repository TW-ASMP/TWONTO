# Breaker SHAPE

tw:Breaker_defaultShape
	a sh:NodeShape ;
	sh:targetClass tw:breaker ;    
	sh:property [
		sh:path tw:breaker_type ; #4Dev: the property label on the form "NEMA enclosure type" is the RDFS:label of this OWL dataProperty.
		sh:name "Breaker Type";
		sh:datatype xsd:string ;
		sh:in ( "Insulated Case" "Metal Clad or Enclosed" "Molded Case") ;
		sh:maxCount 1 ; sh:minCount 1 ;
		sh:message "Value must be a string and should be found in the standard / dropdown value list" ;
    	sh:severity sh:Warning ; #4Dev: this line tells you to allow alternatives
		sh:order 50 ;
	] ;

    sh:property [
		sh:path tw:specialized_valve_type ;
		sh:name "Specialized Valve Type";
		sh:datatype xsd:string ;
		sh:in ("Air Release Valve" "Backflow Preventer" "Check" "Pressure Relief Valve" "Solenoid") ;
		sh:message "Value must be a string and should be found in the standard / dropdown value list" ;
    	sh:severity sh:Warning ;
		sh:order 60 ;
	] ;

	sh:property [
		sh:class tw:Max_Continuous_Voltage_Rating  ;
		sh:name "Max Continuous Voltage Rating ";
		sh:minCount 1 ; sh:maxCount 1 ; 
		sh:severity sh:Violation ;
		sh:order 10 ;
	] ;

	sh:property [
		sh:class tw:Max_Continuous_Current_Rating  ;
		sh:name "Max Continuous Current Rating ";
		sh:minCount 1 ; sh:maxCount 1 ; 
		sh:severity sh:Violation ;
		sh:order 10 ;
	] ;

	sh:property [
		sh:path tw:Breaker_Main_Contractor_Type ;
		sh:name "Breaker Main Contractor Type";
		sh:datatype xsd:string ;
		sh:in ("Air Insulated" "Air Insulated, Air Blast" "Vacuum Insulated" "Oil Insultated" "Gas Insulated") ;
		sh:message "Value must be a string and should be found in the standard / dropdown value list" ;
    	sh:severity sh:Warning ;
		sh:order 60 ;
	] ;

