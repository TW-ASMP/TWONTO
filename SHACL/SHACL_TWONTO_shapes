##################################################################################
# RE-USABLE UNIT OF MEASURE DATA PROPERTY SHAPE DEFINITIONS
##################################################################################

eidf:voltage_UOM 
	a sh:PropertyShape ;
	sh:maxCount 1 ; sh:minCount 1 ;
	sh:path eidf:has_unit_of_measure ;
	sh:datatype xsd:string ;
	sh:in ( "m^2" "in^2" "ft^2" ) ;
	sh:order 20 .

eidf:power_UOM 
	a sh:PropertyShape ;
	sh:maxCount 1 ; sh:minCount 1 ;
	sh:path eidf:has_unit_of_measure ;
	sh:datatype xsd:string ;
	sh:in ( "W" "kW" "VA" "kVA" "MVA" ) ;
	sh:order 20 .

eidf:volumn_UOM 
	a sh:PropertyShape ;
	sh:maxCount 1 ; sh:minCount 1 ;
	sh:path eidf:has_unit_of_measure ;
	sh:datatype xsd:string ;
	sh:in ( "L" "gal" "cm^3" "m^3" "in^3" "ft^3" ) ;
	sh:order 20 .

eidf:temperature_UOM 
	a sh:PropertyShape ;
	sh:maxCount 1 ; sh:minCount 1 ;
	sh:path eidf:has_unit_of_measure ;
	sh:datatype xsd:string ;
	sh:in ( "C." "F." "K." ) ;
	sh:order 20 .

eidf:pressure_UOM 
	a sh:PropertyShape ;
	sh:maxCount 1 ; sh:minCount 1 ;
	sh:path eidf:has_unit_of_measure ;
	sh:datatype xsd:string ;
	sh:in ( "Pa" "kPa" "atm" "ppi" "mmHg" ) ;
	sh:order 20 .

eidf:area_UOM 
	a sh:PropertyShape ;
	sh:maxCount 1 ; sh:minCount 1 ;
	sh:path eidf:has_unit_of_measure ;
	sh:datatype xsd:string ;
	sh:in ( "in^2" "cm^2" "m^2" "ft^2" ) ;
	sh:order 20 .
