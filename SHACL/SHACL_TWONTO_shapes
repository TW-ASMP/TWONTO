		sh:severity sh:Violation ;
		sh:order 10 ;
	] ;

	sh:property [ 
		sh:class "compressor_drive"
		sh:name "Compressor Drive Coupling" ;
		sh:datatype xsd:string ;
		sh:in ( "True" "False") ;
