# RE-USABLE PROPERTY SHAPE DEFINITIONS
##################################################################################

tw:voltageSpec_nodeShape
	a sh:NodeShape;
	sh:property [
		sh:path tw:isQuantifiedBy ;
		sh:class tw:measurement ;
		sh:minCount 1 ;
		sh:node [
			sh:property [
				sh:maxCount 1 ; 
				sh:minCount 1 ;
				sh:path tw:hasNumericValue ;
				sh:datatype xsd:float ;
				sh:order 10 ;
			];
			sh:property eidf:voltage_UOM;
		];
	] .

